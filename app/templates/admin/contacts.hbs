<!-- app/templates/admin/invitations.hbs -->

<h1>Contacts</h1>

<table class="table table-bordered table-striped">
    <thead>
      <tr>
          <th>email</th>
          <th>message</th>
      </tr>
    </thead>
    <tbody>
    {{#each model as |contact|}}
        <tr>
            <th>{{contact.email}}</th>
            <td>{{contact.message}}</td>
        </tr>
    {{/each}}

    </tbody>
</table>
